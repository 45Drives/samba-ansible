[logging]
  default = FILE:/var/log/krb5libs.log
  kdc = FILE:/var/log/krb5kdc.log
  admin_server = FILE:/var/log/kadmind.log

[libdefaults]
  dns_lookup_realm = false
  dns_lookup_kdc = true
  default_realm = {{ realm|upper }}
  ticket_lifetime = 24h
  renew_lifetime = 7d
  forwardable = true
  rdns = false

[realms]

[domain_realm]
  .{{ realm|lower }} = {{ realm|upper }}
  {{ realm|lower }} = {{ realm|upper }}
